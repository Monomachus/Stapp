<h1>Listing product_operations</h1>

<table>
  <tr>
    <th>User</th>
    <th>Product</th>
    <th>Operation type</th>
    <th>Product number</th>
    <th>Operation date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_operations.each do |product_operation| %>
  <tr>
    <td><%= product_operation.user_id %></td>
    <td><%= product_operation.product_id %></td>
    <td><%= product_operation.operation_type_id %></td>
    <td><%= product_operation.product_number %></td>
    <td><%= product_operation.operation_date %></td>
    <td><%= link_to 'Show', product_operation %></td>
    <td><%= link_to 'Edit', edit_product_operation_path(product_operation) %></td>
    <td><%= link_to 'Destroy', product_operation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

